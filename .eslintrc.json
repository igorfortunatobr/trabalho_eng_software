{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "off",
    "no-async-promise-executor": "off"
  },
  "globals": {
    "Util": true,
    "connection": true,
    "RequestResult": true,
    "FormatSQLRes": true,
    "Constantes": true,
    "sumArray": true,
    "async": true,
    "horarioBrasilia": true,
    "session": true
  }
}
